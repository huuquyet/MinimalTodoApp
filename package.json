{
  "name": "minimaltodo",
  "version": "0.2.6",
  "homepage": "https://huuquyet.github.io/MinimalTodoApp/",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "android": "yarn e:android",
    "build:all": "cd $INIT_CWD && yarn workspaces foreach --all --exclude next-app run build",
    "check:expo": "yarn workspace expo-app expo install --check",
    "clean": "yarn cache clean --all && rm -rf apps/next/.tamagui yarn.lock && npx npkill",
    "doctor": "yarn dlx @yarnpkg/doctor && yarn e:doctor",
    "ios": "yarn e:ios",
    "native": "yarn e:start",
    "lint": "yarn dlx @biomejs/biome check --apply .",
    "postinstall": "yarn build:all",
    "upgrade:tamagui": "yarn up '*tamagui*'@latest '@tamagui/*'@latest",
    "upgrade:tamagui:canary": "yarn up '*tamagui*'@canary '@tamagui/*'@canary",
    "vercel:install": "yarn set version berry && yarn install",
    "watch": "yarn workspaces foreach -pi run watch",
    "web": "yarn build:all && yarn workspace next-app run next",
    "web:extract": "DISABLE_EXTRACTION=false yarn n:dev",
    "web:prod": "yarn n:build",
    "web:prod:serve": "yarn n:serve"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.3.3",
    "turbo": "^1.10.16",
    "typescript": "^5.2.2"
  },
  "packageManager": "yarn@4.0.1"
}
